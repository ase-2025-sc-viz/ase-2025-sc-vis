<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
     <view
            category="org.eclipse.linuxtools.tmf.ui.views.category"
            class="org.eclipse.tracecompass.analysis.profiling.ui.views.flamechart.FlameChartView"
            icon="icons/eview16/callstack_view.gif"
            id="org.eclipse.linuxtools.tmf.ui.views.callstack"
            name="%view.flamechart.name"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="org.eclipse.linuxtools.tmf.ui.views.category"
            class="org.eclipse.tracecompass.internal.analysis.profiling.ui.callgraph.CallGraphDensityView"
            icon="icons/eview16/funcdensity.png"
            id="org.eclipse.tracecompass.internal.analysis.timing.ui.callgraph.callgraphDensity"
            name="%view.callgraphDensity"
            restorable="true">
      </view>
      <view
            category="org.eclipse.linuxtools.tmf.ui.views.category"
            class="org.eclipse.tracecompass.internal.analysis.profiling.ui.flamegraph.FlameGraphView"
            icon="icons/eview16/flame.png"
            id="org.eclipse.tracecompass.internal.analysis.timing.ui.flamegraph.flamegraphView"
            name="%view.flameGraph"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="org.eclipse.linuxtools.tmf.ui.views.category"
            class="org.eclipse.tracecompass.internal.analysis.profiling.ui.callgraph.statistics.CallGraphStatisticsView"
            icon="icons/eview16/statistics_view.gif"
            id="org.eclipse.tracecompass.internal.analysis.timing.ui.callgraph.statistics.callgraphstatistics"
            name="%view.callgraph.statistics"
            restorable="true">
      </view>
      <view
            category="org.eclipse.linuxtools.tmf.ui.views.category"
            class="org.eclipse.tracecompass.internal.analysis.profiling.ui.flamegraph2.FlameGraphView"
            icon="icons/elcl16/flame.png"
            id="org.eclipse.tracecompass.analysis.profiling.ui.flamegraph"
            name="%view.flameGraph2"
            restorable="true">
      </view>
      <view
            category="org.eclipse.linuxtools.tmf.ui.views.category"
            class="org.eclipse.tracecompass.internal.analysis.profiling.ui.flamegraph2.FlameGraphSelView"
            icon="icons/elcl16/flame.png"
            id="org.eclipse.tracecompass.analysis.profiling.ui.flamegraph.selection"
            name="%view.flameGraphSel"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="org.eclipse.linuxtools.tmf.ui.views.category"
            class="org.eclipse.tracecompass.internal.analysis.profiling.ui.functiondensity.FunctionDensityView"
            icon="icons/elcl16/funcdensity.png"
            id="org.eclipse.tracecompass.analysis.profiling.ui.functiondensity"
            name="%view.functionDensity"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            category="org.eclipse.linuxtools.tmf.ui.views.category"
            class="org.eclipse.tracecompass.internal.analysis.profiling.ui.weightedtree.WeightedTreeView"
            icon="icons/obj16/stckframe_obj.gif"
            id="org.eclipse.tracecompass.analysis.profiling.ui.weightedtree"
            name="%view.weighted"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            category="org.eclipse.linuxtools.tmf.ui.views.category"
            class="org.eclipse.tracecompass.internal.analysis.profiling.ui.FlameChartView"
            icon="icons/obj16/stckframe_obj.gif"
            id="org.eclipse.tracecompass.analysis.profiling.ui.flamechart"
            name="%view.flameChart"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.linuxtools.tmf.core.analysis">
      <listener
            class="org.eclipse.tracecompass.internal.analysis.profiling.ui.views.flamechart.CallStackAnalysisListener">
      </listener>
      <listener
            class="org.eclipse.tracecompass.internal.analysis.profiling.ui.CallStackAnalysisListener">
      </listener>
   </extension>
   <extension
         point="org.eclipse.tracecompass.tmf.core.symbolProvider">
      <providerFactory
            class="org.eclipse.tracecompass.internal.analysis.profiling.ui.symbols.BasicSymbolProviderFactory"
            id="org.eclipse.tracecompass.analysis.profiling.ui.providerFactory.basic"
            priority="0">
      </providerFactory>
   </extension>
</plugin>
